{
  "name": "gh-pages-demo",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://YOUR_USERNAME.github.io/gh-pages-demo",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "serve": "json-server --watch ./src/server/db/data.json --port 3001",
    "save-json-api": "node ./src/server/scripts/save-json-api.js",
    "build": "react-scripts build && npm run save-json-api && shx cp build/index.html build/404.html",
    "predeploy": "rm -rf build && npm run build",
    "deploy": "gh-pages -d build",
    "test": "echo \"Tests disabled\" && exit 0",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "gh-pages": "^6.3.0",
    "json-server": "^0.17.0",
    "node-fs": "^0.1.7",
    "shx": "^0.3.4"
  }
}
